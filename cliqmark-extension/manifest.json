{

  "name": "cliqmark plugin",
  "description": "bookmark manager",
  "version": "1.0",

  "permissions": [
   "tabs",
   "storage",
   "<all_urls>"
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "content_scripts": [
    { 
      "matches": ["<all_urls>"],
      "js": ["bookmark.js"]
    }
  ],

  "manifest_version": 2
}